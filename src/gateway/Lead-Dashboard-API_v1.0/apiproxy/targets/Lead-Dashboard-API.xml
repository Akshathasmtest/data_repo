<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="Lead-Dashboard-API">
    <Description>Lead-Dashboard-API</Description>
	<FaultRules>
	</FaultRules>
	<DefaultFaultRule>
            <Step>
                <Name>SF-PostflowResponseTargetLogAggregator</Name>
            </Step>
		<Step>
            <Name>SF-Noname-Security</Name>
    	</Step>
    	<AlwaysEnforce>true</AlwaysEnforce>
	</DefaultFaultRule>
	<PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>SF-PreflowRequestTargetLogAggregator</Name>
            </Step>
            <Step>
                <Name>cf-Remove-Proxy-Auth-Header</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
	<Flows>
        <Flow name="band">
        	<Condition>(targetOperation = &quot;band&quot;)</Condition>
			<Request>
			</Request>   
			<Response>
			</Response>
        </Flow>
        <Flow name="GET_attendance_overview_today">
        	<Condition>(targetOperation = &quot;GET_attendance_overview_today&quot;)</Condition>
			<Request>
			</Request>   
			<Response>
			</Response>
        </Flow>
        <Flow name="validate-money">
        	<Condition>(targetOperation = &quot;validate-money&quot;)</Condition>
			<Request>
			</Request>   
			<Response>
			</Response>
        </Flow>
	</Flows>
    <PostFlow name="PostFlow">
        <Request>
            <Step>
                <Name>SF-PostflowRequestTargetLogAggregator</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>SF-PostflowResponseTargetLogAggregator</Name>
            </Step>
        </Response>
    </PostFlow>
    <HTTPTargetConnection>
    	<LoadBalancer>
            <Server name="Lead-Dashboard-API">
                <IsEnabled>true</IsEnabled>
            </Server>
        </LoadBalancer>
        <Path>{targetBasePath}</Path>
    </HTTPTargetConnection>
</TargetEndpoint>
